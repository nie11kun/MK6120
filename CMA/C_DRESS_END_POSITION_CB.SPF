PROC C_DRESS_END_POSITION_CB DISPLOF
;***********程序功能**********
;*修整结束移动位置
;****************************

;
MSG("砂轮架离开中"); ifIsCHS
G1 G90 Y=WHEEL[10]+LADAO[92]+1 F=INI[55];x轴回退到修整接触位+2
;
MSG("台面离开中"); ifIsCHS
G1 G90 Z=DRESSER[33]+LADAO[184] F=INI[56];z轴快速接近

IF GRIND[1]<>0;纯修整
    RET;
ENDIF

G90 G01 Y=INI[23] F=INI[55];Y轴到安全位置
G90 G01 X=LADAO[12] F=LADAO[2];X轴到安全位置

C_ROTATE_ANGLE(INI[20],LADAO[5]);螺旋升角

G90 G1 Z=INI[6] F=INI[56]

RET

