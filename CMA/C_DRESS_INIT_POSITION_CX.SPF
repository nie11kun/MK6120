PROC C_DRESS_INIT_POSITION_CX DISPLOF
;***********程序功能**********
;*修整初始移动位置
;****************************

MSG("台面到位中"); ifIsCHS
G90 G1 Z=DRESSER[21]+150 F=INI[56]
;
MSG("修整轮运行至起始位置"); ifIsCHS
G90 G1 Y=WHEEL[10]+INI[132] F=INI[55];多退刀一点，防止齿高过大抬刀不够

C_RESET_ANGLE;升角复位

G90 G1 X=LADAO[4] F=LADAO[2]
;
MSG("台面到位中"); ifIsCHS
G90 G1 Z=DRESSER[21] F=INI[56]

;
MSG("修整轮运行至修整位置"); ifIsCHS
G90 G1 Y=WHEEL[10]+1 F=INI[55]
G90 G1 Y=WHEEL[10]+0.1 F=200

RET

