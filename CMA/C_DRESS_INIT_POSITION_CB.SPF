PROC C_DRESS_INIT_POSITION_CB DISPLOF
;***********程序功能**********
;*修整初始移动位置
;****************************

MSG("台面到位中"); ifIsCHS
G1 G90 Z=DRESSER[33]+DRESSER[22]/2+150 F=INI[56];z轴快速接近
;
C_ROTATE_ANGLE(INI[153],LADAO[6]);升角复位

MSG("修整轮运行至起始位置"); ifIsCHS
G1 G90 Y=WHEEL[10]+2 F=INI[55];x轴回退到修整接触位+2
;
G90 G1 X=LADAO[4] F=LADAO[2]

MSG("台面到位中"); ifIsCHS
G1 G90 Z=DRESSER[33]+DRESSER[22]/2+5 F=INI[56];z轴快速接近

RET

