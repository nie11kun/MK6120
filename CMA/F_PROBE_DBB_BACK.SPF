PROC F_PROBE_DBB_BACK(REAL PROBE_CHOICE) DISPLOF
;***********程序功能**********
;*测量机构缩回到位DBB信号
;PROBE_CHOICE:测量机构1/2
;****************************

IF INI[157]==0;测头伸出缩回是否有检测信号(0有1没有)
    IF PROBE_CHOICE==0;测量机构1
        WHILE($A_DBB[7]==0);等待接近开关到位
            G4 F0.2
        ENDWHILE
    ELSE
        WHILE($A_DBB[20]==0);等待测量头到位
            G4 F0.2
        ENDWHILE
    ENDIF
ELSE
    G4 F3
ENDIF

RET

