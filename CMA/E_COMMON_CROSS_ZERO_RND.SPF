PROC E_COMMON_CROSS_ZERO_RND DISPLOF
;***********程序功能**********
;*左右第一條縣长度为0且无圆度情况，修尖角模式
;****************************

IF (COMMON_LENGTH[1]==0) AND (COMMON_LENGTH[11]==0)
    COMMON_LENGTH[1]=COMMON_LENGTH[2]/2
    COMMON_LENGTH[2]=COMMON_LENGTH[2]/2
    COMMON_LENGTH[11]=COMMON_LENGTH[12]/2
    COMMON_LENGTH[12]=COMMON_LENGTH[12]/2
    COMMON_ANG[1]=COMMON_ANG[2]
    COMMON_ANG[11]=COMMON_ANG[12]
    COMMON_RND[1]=0
    COMMON_RND[11]=0
ENDIF

RET

