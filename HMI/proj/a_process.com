				VAR38.VAL=VAR34.VAL*PI*HOUDU_DIA.VAL/60000
				VAR39.VAL=VAR35.VAL*PI*HOUDU_DIA.VAL/60000
				VAR40.VAL=VAR36.VAL*PI*HOUDU_DIA.VAL/60000
			ENDIF
		ENDIF
	END_SUB

